<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="empleado">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="nombre" type="xs:string" />
				<xs:element name="apellido" type="xs:string" />
				<xs:element name="numPersonal" type="xs:integer" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>